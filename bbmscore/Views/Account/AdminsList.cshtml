
<h2>Admin List</h2>
<table class="table mb-4 mt-4 table-bordered">
    <thead>
        <tr>
            <th>Username</th>
            <th>Email</th>
            <th>Phone</th>
           
            <th>    </th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (var admin in Model)
        {
            <tr>
                <td>@admin.UserName</td>
                <td>@admin.Email</td>
                <td>@admin.PhoneNumber</td>
               
                <td><a class="text-decoration-none btn btn-danger">Delete</a></td>
                
            </tr>
        }
    </tbody>
   
</table>
<div class="text-center">
    <a class="text-decoration-none btn btn-success" asp-action="AddAdmins" asp-controller="Account" >Add Admins</a>
    <a class="text-decoration-none btn btn-dark" asp-action="Index" asp-controller="Home">Back</a>
</div>